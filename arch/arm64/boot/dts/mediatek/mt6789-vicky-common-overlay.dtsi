#include <dt-bindings/gpio/gpio.h>
#include "mediatek/mt6789-moto-common-overlay.dtsi"
#include "mediatek/mt6789_vicky_camera_overlay.dtsi"
#include "mediatek/vicky-display.dtsi"
#include "mediatek/mt6789-vicky-audio-overlay.dtsi"
#include "mediatek/vicky-touch.dtsi"
#include "mediatek/mt6789-vicky-fps-overlay.dtsi"
&dispsys_config {
	pinctrl-names =
		"lcm_rst_out1_gpio", "lcm_rst_out0_gpio",
		"mode_te_te";
	pinctrl-0 = <&mtkfb_pins_lcm_rst_out1_gpio>;
	pinctrl-1 = <&mtkfb_pins_lcm_rst_out0_gpio>;
	pinctrl-2 = <&mtkfb_pins_lcm_dsi_te>;
	status = "okay";
};


/* GPS GPIO standardization start */
&pio {
	gps_pins_default: gps_default {
	};
	gps_l1_lna_pins_ol: gps_l1_lna@0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPIO25>;
			output-low;
		};
	};
	gps_l1_lna_pins_dsp_ctrl: gps_l1_lna@1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPS_L1_ELNA_EN>;
		};
	};
	gps_l1_lna_pins_oh: gps_l1_lna@2 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO25__FUNC_GPIO25>;
			output-high;
		};
	};
	gps_l5_lna_pins_ol: gps_l5_lna@0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
			output-low;
		};
	};
	gps_l5_lna_pins_dsp_ctrl: gps_l5_lna@1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPS_L5_ELNA_EN>;
		};
	};
	gps_l5_lna_pins_oh: gps_l5_lna@2 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
			output-high;
		};
	};
};
&gps {
	pinctrl-names = "default",
		"gps_l1_lna_disable",
		"gps_l1_lna_dsp_ctrl",
		"gps_l1_lna_enable",
		"gps_l5_lna_disable",
		"gps_l5_lna_dsp_ctrl",
		"gps_l5_lna_enable";
	pinctrl-0 = <&gps_pins_default>;
	pinctrl-1 = <&gps_l1_lna_pins_ol>;
	pinctrl-2 = <&gps_l1_lna_pins_dsp_ctrl>;
	pinctrl-3 = <&gps_l1_lna_pins_oh>;
	pinctrl-4 = <&gps_l5_lna_pins_ol>;
	pinctrl-5 = <&gps_l5_lna_pins_dsp_ctrl>;
	pinctrl-6 = <&gps_l5_lna_pins_oh>;
	status = "okay";
};
/* GPS GPIO standardization end */

#include "mediatek/cust_mt6789_camera_v4l2.dtsi"

&i2c5 {
	i2c_camera_ldo2{
		compatible = "mediatek,i2c_camera_ldo2";
		reg = <0x2f>;
		status = "okay";
	};
	i2c_camera_ldo{
		compatible = "mediatek,i2c_camera_ldo";
		reg = <0x35>;
		status = "okay";
	};
	aw36518@63 {
		compatible = "awinic,aw36518";
		reg = <0x63>;
		status = "okay";
	};
};


&i2c6 {
	spk: speaker_amp@5c {
		compatible = "richtek,rt5512";
		#sound-dai-cells = <0>;
		reg = <0x5c>;
		status = "okay";
		sound-name-prefix = "Left";
	};
};


&i2c0 {
	status = "okay";
	aw9610x_sar@12 {
		compatible = "awinic,aw9610x_sar_0";
		reg = <0x12>;
		irq-gpio = <&pio 13 0x0>;
                chip_name = "AW96105";
		sar-num = <0>;
		aw9610x,channel_number = <0x3>;
		ch0_name = "capsense_bottom_left";
		ch1_name = "capsense_top";
		ch2_name = "capsense_bottom_right";
		aw9610x,using-firmware;
                aw9610x,firmware_name = "aw9610x_0";
		status = "okay";
	};
};

&i2c3 {
       nfc_pn5xx: nfc_pn5xx@28 {
		compatible = "nxp,pn5xx";
		reg = <0x28>;
		interrupt-parent = <&pio>;
		interrupts = <1 IRQ_TYPE_LEVEL_HIGH 1 0>;
		nxp,pn544-irq = <&pio 1 0>;
		nxp,pn544-ven = <&pio 150 0>;
		nxp,pn544-fd = <&pio 99 0>;
		status = "okay";
	};
};

&sound {
	mediatek,spk-i2s = <3 0>;
	mediatek,speaker-codec {
		sound-dai = <&spk>;
	};
};

/* sdcard */
&mmc1 {
	cd-gpios = <&pio 7 GPIO_ACTIVE_HIGH>;
};

&mtk_ctd {
		bootmode = <&chosen>;
};

/* SVP */
&mtee_svp {
	mtee: MTEE {
		compatible = "mediatek,mtk_svp_on_mtee_support";
	};
	svp: SecureVideoPath {
		compatible = "mediatek,mtk_sec_video_path_support";
	};
};

