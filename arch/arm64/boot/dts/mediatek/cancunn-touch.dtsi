/* TOUCH start */

&spi3 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	touch@0{
		compatible = "focaltech,fts", "tchip,ilitek", "chipone-tddi";
		reg = <0>; //Same as CS ID
		status = "ok";

		spi-max-frequency = <9600000>;	//4800000,9600000,15000000,19200000

		//FTS
		focaltech,reset-gpio = <&pio 3 0x0>;
		focaltech,irq-gpio = <&pio 22 0x0>;
		focaltech,max-touch-number = <10>;
		focaltech,display-coords = <0 0 1080 2400>;
		focaltech,report_gesture_key = <59>;
		focaltech,panel-supplier = "djn";

		//ilitek
		touch,reset-gpio = <&pio 3 0x0>;
		touch,irq-gpio = <&pio 22 0x0>;
		touch,psy-name = "battery";
		//ilitek,report_gesture_key;
		//ilitek,supported_gesture_type = <0x06>; //bit2-0: double:single:zero tap

		//chipone
		chipone,rst-gpio = <&pio 3 0x0>;
		chipone,irq-gpio = <&pio 22 0x0>;
		chipone,x-res = <1080>;
		chipone,y-res = <2400>;

	        //chipone,def-build-id = <0x0101>;
	        //chipone,def-config-id = <0x0001>;
		//chipone,supported_gesture_type = <0x06>;

	};
};

/* TOUCH end */
