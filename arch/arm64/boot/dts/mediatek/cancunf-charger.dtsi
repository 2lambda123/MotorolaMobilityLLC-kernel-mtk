&charge_leds {
	compatible = "gpio-leds";

	charging {
		label = "charging";
		gpios = <&pio 23 0x0>;
		default-state = "off";
	};
};

&charger_cooler {
	bootmode = <&chosen>;
	mmi,thermal-mitigation = <(-1) 5200000 4700000 4000000 3500000
				3000000 1000000 500000 300000>;
};


&ap_ntc {
	polling-delay = <5000>;
	polling-delay-passive = <3000>;
 	thermal-sensors = <&tboard_thermistor1>;

	trips {
		charge_trip0: charge-trip0 {
			temperature = <42000>;
			hysteresis = <2000>;
			type = "passive";
		};
		charge_trip1: charge-trip1 {
			temperature = <43000>;
			hysteresis = <1000>;
			type = "passive";
		};
		charge_trip2: charge-trip2 {
			temperature = <44000>;
			hysteresis = <1000>;
			type = "passive";
		};
		charge_trip3: charge-trip3 {
			temperature = <45000>;
			hysteresis = <1000>;
			type = "passive";
		};
		charge_trip4: charge-trip4 {
			temperature = <46000>;
			hysteresis = <1000>;
			type = "passive";
		};
		charge_trip5: charge-trip5 {
			temperature = <47000>;
			hysteresis = <1000>;
			type = "passive";
		};
		charge_trip6: charge-trip6 {
			temperature = <48000>;
			hysteresis = <1000>;
			type = "passive";
		};
	};

	cooling-maps {
		charge-cdev0 {
			trip = <&charge_trip0>;
			cooling-device = <&charger_cooler 1 1>;
		};
		charge-cdev1 {
			trip = <&charge_trip1>;
			cooling-device = <&charger_cooler 2 2>;
		};
		charge-cdev2 {
			trip = <&charge_trip2>;
			cooling-device = <&charger_cooler 3 3>;
		};
		charge-cdev3 {
			trip = <&charge_trip3>;
			cooling-device = <&charger_cooler 4 4>;
		};
		charge-cdev4 {
			trip = <&charge_trip4>;
			cooling-device = <&charger_cooler 5 5>;
		};
		charge-cdev5 {
			trip = <&charge_trip5>;
			cooling-device = <&charger_cooler 6 6>;
		};
		charge-cdev6 {
			trip = <&charge_trip6>;
			cooling-device = <&charger_cooler 7 7>;
		};
	};

};


&moto_chg_tcmd {
	compatible = "mmi,moto-chg-tcmd";
	status = "okay";

	mmi,ntc_table = <1>;
	mmi,ntc_v_ref = <1800000>;
	mmi,ntc_r_pull = <100000>;

	mmi,batid_v_ref = <1800000>;
	mmi,batid_r_pull = <100000>;

	mmi,adc-channel-cpu = <0>;
	mmi,adc-channel-charger = <2>;
	mmi,adc-channel-pa = <1>;
	mmi,adc-channel-batid = <3>;
	mmi,adc-channel-vbat = <100>;
};

&lk_charger {
       /* LCD brightness*/
       led_brightness = <1023>;
};
